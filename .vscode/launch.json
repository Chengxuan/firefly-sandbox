{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run server tests",
      "cwd": "${workspaceFolder}/server",
      "runtimeExecutable": "npm",
      "args": ["run", "test"],
      "request": "launch",
      "type": "node",
      "outputCapture": "std"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/ui"
    }
  ]
}
